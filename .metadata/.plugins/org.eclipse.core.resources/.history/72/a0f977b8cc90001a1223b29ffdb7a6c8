package thai.dto;


import java.sql.Date;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor
public class StudentDTO {

	private int id;
	
	private String name;
	
	private String code;
	
	private Date birthday;
	
	private String address;
	
	private double average;
	
}
