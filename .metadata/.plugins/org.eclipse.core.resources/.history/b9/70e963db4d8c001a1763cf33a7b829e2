package thai.entity;

import javax.validation.constraints.NotNull;


import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor
public class User {
	
	@NotNull
	private Integer id;
	
	private String name;
	
	private String password;
	
	
	
}
